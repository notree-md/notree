{
  "name": "@mindgraph/draw",
  "version": "0.2.0",
  "description": "web based graph view for markdown notes",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint src/**",
    "typecheck": "tsc --noEmit",
    "dev": "node example/server.js"
  },
  "type": "module",
  "author": "jollyjerr <jollyjerr@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/draw.umd.cjs",
  "module": "./dist/draw.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/draw.js",
      "require": "./dist/draw.umd.cjs"
    }
  },
  "devDependencies": {
    "@mindgraph/read": "workspace:*",
    "@mindgraph/types": "workspace:*",
    "@types/d3-array": "^3.0.4",
    "@types/d3-force": "^3.0.4",
    "@types/d3-selection": "^3.0.4",
    "@types/d3-zoom": "^3.0.2",
    "express": "^4.18.2",
    "vite": "^4.0.0",
    "vitest": "^0.27.0"
  },
  "dependencies": {
    "d3-array": "^3.2.1",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0"
  }
}
